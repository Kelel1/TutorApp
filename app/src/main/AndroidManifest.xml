<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tutorapp">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".signUp1"></activity>
        <activity
            android:name=".GiveFeedback"
            android:label="@string/title_activity_give_feedback"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RateTutor"
            android:label="@string/title_activity_rate_tutor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RescheduleSession"
            android:label="@string/title_activity_reschedule_session"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CancelSession"
            android:label="@string/title_activity_cancel_session"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ConfirmSession"
            android:label="@string/title_activity_confirm_session"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScheduleSession"
            android:label="@string/title_activity_schedule_session"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SearchTutors"
            android:label="@string/title_activity_search_tutors"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RemoveCourse"
            android:label="@string/title_activity_remove_course"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddCourse"
            android:label="@string/title_activity_add_course"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".test"
            android:label="@string/title_activity_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SignUp"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".Dashboard"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".MainActivity" />


    </application>

</manifest>